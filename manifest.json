{
	"name": "Check for vaccination appointments",
	"version": "1.0",
	"manifest_version": 3,
	"content_scripts": [
		{
			"js": [
				"index.js"
			],
			"matches": [
				"https://*.impfterminservice.de/*"
			]
		}
